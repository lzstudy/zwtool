NOTE_ROOT=/root/zw_note

########################################################################################################
# 程序入口
########################################################################################################
function websync_entry()
{
    # 进入目录
    cd ${NOTE_ROOT}

    # 部署网站
    if [ $# -lt 1 ];then
        ./deploy note_index
    else
        ./deploy note_linux_$1
    fi
}

websync_entry $@
